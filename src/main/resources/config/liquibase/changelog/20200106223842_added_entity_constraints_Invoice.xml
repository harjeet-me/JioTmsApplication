<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Invoice.
    -->
    <changeSet id="20200106223842-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ref1_id"
                                 baseTableName="invoice"
                                 constraintName="fk_invoice_ref1_id"
                                 referencedColumnNames="id"
                                 referencedTableName="invoice_ref"/>

        <addForeignKeyConstraint baseColumnNames="ref2_id"
                                 baseTableName="invoice"
                                 constraintName="fk_invoice_ref2_id"
                                 referencedColumnNames="id"
                                 referencedTableName="invoice_ref"/>

        <addForeignKeyConstraint baseColumnNames="ref3_id"
                                 baseTableName="invoice"
                                 constraintName="fk_invoice_ref3_id"
                                 referencedColumnNames="id"
                                 referencedTableName="invoice_ref"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="invoice"
                                 constraintName="fk_invoice_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>

        <addForeignKeyConstraint baseColumnNames="trip_id"
                                 baseTableName="invoice"
                                 constraintName="fk_invoice_trip_id"
                                 referencedColumnNames="id"
                                 referencedTableName="trip"/>

    </changeSet>
</databaseChangeLog>
